<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts
Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd" >
<struts>

	<constant name="struts.devMode" value="true" />
<!-- 	<constant name="struts.objectFactory" value="org.apache.struts2.spring.StrutsSpringObjectFactory" /> -->
	
	<package name="default" namespace="/" extends="struts-default">
		<action name="">
			<result name="success">/pages/index.jsp</result>
		</action>
		
<!-- LOGIN -->
		<action name="login" class="ar.edu.unlp.info.infopool.actions.LoginAction">
			<result name="success">/pages/login.jsp</result>
		</action>
	
		<action name="loginProcess" class="ar.edu.unlp.info.infopool.actions.LoginAction" method="autenticate">
			<result name="success">/pages/main.jsp</result>
			<result name="badlogin">/pages/login.jsp</result>
			<result name="logged">/pages/main.jsp</result>
		</action>



	</package>

</struts>



